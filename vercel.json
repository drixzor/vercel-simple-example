{
  "version": 2,
  "builds": [
    {
      "src": "api/hello.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/fetchBookings.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/fetchSessions.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/hello",
      "dest": "api/hello.js"
    },
    {
      "src": "/api/fetchBookings",
      "dest": "api/fetchBookings.js"
    },
    {
      "src": "/api/fetchSessions",
      "dest": "api/fetchSessions.js"
    }
  ]
}
